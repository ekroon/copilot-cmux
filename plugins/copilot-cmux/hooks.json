{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "type": "command",
        "bash": "python3 ./scripts/cmux_notify.py sessionStart",
        "timeoutSec": 10
      }
    ],
    "preToolUse": [
      {
        "type": "command",
        "bash": "python3 ./scripts/cmux_notify.py preToolUse",
        "timeoutSec": 10
      }
    ],
    "sessionEnd": [
      {
        "type": "command",
        "bash": "python3 ./scripts/cmux_notify.py sessionEnd",
        "timeoutSec": 10
      }
    ]
  }
}
